@page
@model LogInPrefab.Pages.AccountPages.LogInModel
@{
}
<div class="row">
    <div class="col-6 ">
        <div class="card" id="card">
            <div class="card-header">
                <h2>Log in as an existing user</h2>
            </div>
            <div class="card-body">
                <form method="post">
                    <div class="mb-3">
                        <label class="form-label">User name</label>
                        <input asp-for="Account.AccountName" id="userName" name="userName" type="text" class="form-control" />
                    </div>


                    <div class="mb-3">
                        <label class="form-label">password</label>
                        <input id="password" name="password" type="password" class="form-control" />
                    </div>
                    <button type="submit" class="btn btn-primary mt-5">Log in</button>

                </form>
            </div>
            <div class="card-footer"></div>
        </div>
    </div>
</div>